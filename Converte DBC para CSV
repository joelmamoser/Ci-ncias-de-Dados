#ativa a lib
library("read.dbc")

#####
# converter arquivos dbc em csv
# leio o dbc
x <- read.dbc("C:/Users/Jo/Desktop/RENAL/PASC2201.dbc")
#escrevo o csv
write.csv(x,file="PASC2201_JO.csv")

